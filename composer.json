        "doctrine/common": "~2.4",
        "doctrine/doctrine-module": "0.*"
    },
    "require-dev": {
        "zendframework/zendframework": "~2.2",
        "phpunit/phpunit": "~3.7",
        "squizlabs/php_codesniffer": "1.4.*",
